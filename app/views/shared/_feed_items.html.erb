<li id="<%= feed_items.id %>">
  <ul class="inline-list">
    <li><%= link_to gravatar_for(feed_items.user), feed_items.user %></li>
    <li><%= link_to feed_items.user.name, feed_items.user %>Posted <%= time_ago_in_words(feed_items.created_at) %> ago.</li>
    <li><span class="content"><%= feed_items.content %></span></li>
  </ul>

<hr>
</li>